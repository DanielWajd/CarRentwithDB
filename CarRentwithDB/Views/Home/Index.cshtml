@{
    ViewData["Title"] = "Car rent";
}

<section class="py-5 text-center bg-image" style="background-image: url('https://idsb.tmgrup.com.tr/ly/uploads/images/2022/09/13/230591.jpg'); background-size: cover; background-position: center; background-repeat: no-repeat; width: 100vw; margin-left: calc(50% - 50vw); min-height: 600px;" ">
    <div class="row py-lg-5">
        <div class="col-lg-6 col-md-8 mx-auto text-white">
            <h1 class="fw-light">Album example</h1>
            <p class="lead">Something short and leading about the collection below—its contents, the creator, etc. Make it short and sweet, but not too short so folks don’t simply skip over it entirely.</p>
            <form class="d-flex justify-content-center mt-4" role="search">
                <input class="form-control me-2" type="search" placeholder="Szukaj..." aria-label="Search" style="max-width: 400px;">
                <button class="btn btn-primary" type="submit">Szukaj</button>
            </form>
        </div>
    </div>
</section>
